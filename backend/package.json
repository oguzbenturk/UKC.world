{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --ignore '*.test.js' --ignore '*.spec.js' --exec \"node server.js\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch --detectOpenHandles",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage --detectOpenHandles --forceExit",
    "build": "npm run build",
    "deploy": "npm run deploy",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "predeploy": "npm run lint && npm test",
    "postdeploy": "npm run migrate && npm run seed",
    "consistency:check": "node scripts/monitor-soft-delete-consistency.js",
    "db:reset": "node scripts/reset-database.js --force",
    "db:reset:dry": "node scripts/reset-database.js --dry-run",
    "db:reset:wipe-users": "node scripts/reset-database.js --force --wipe-users",
    "services:upsert:lessons": "node scripts/upsert-lesson-services.js",
    "services:upsert:lessons:dry": "node scripts/upsert-lesson-services.js --dry",
    "packages:upsert:lessons": "node scripts/upsert-lesson-packages.js",
    "packages:upsert:lessons:dry": "node scripts/upsert-lesson-packages.js --dry"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@slack/webhook": "^7.0.6",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "iyzipay": "^2.0.64",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.16.0",
    "prom-client": "^15.1.3",
    "redis": "^4.7.1",
    "sharp": "^0.33.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.2.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "chai": "^5.2.0",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1"
  }
}
